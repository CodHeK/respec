{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "last 1 Firefox version, last 1 Chrome version, last 1 Safari version, last 1 Edge version",
        "modules": "amd"
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        ["istanbul", {
          "exclude": [
            "tests/**/*-spec.js"
          ]
        }]
      ]
    }
  }
}
